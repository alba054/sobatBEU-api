{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "radix": 0,
        "new-cap": 0,
        "consistent-return": 0,
        "no-console": 0,
        "no-unused-vars": 0,
        "no-shadow": 0,
        "no-multiple-empty-lines": 0,
        "padded-blocks": 0,
        "no-param-reassign": 0,
        "no-underscore-dangle": 0
    }
}
