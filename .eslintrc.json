{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "radix": 0,
        "new-cap": 0,
        "consistent-return": 0,
        "no-console": 0,
        "no-unused-vars": 0,
        "no-shadow": 0
    }
}
